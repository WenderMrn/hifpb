<div class="row">
  <div class="col-lg-12">
    <h1 class="page-header">Permissões</h1>
  </div>
</div><!--/.row-->

<div class="row">
  <div class="col-lg-12">
    <div class="panel panel-default">
      <div class="panel-heading">
        <div class="row">
          <div class="col-md-12">
            <svg class="glyph stroked pencil"><use xlink:href="#stroked-pencil"/></svg> Salas
          </div>
        </div>
      </div>
      <div class="panel-body">

        <table data-toggle="table" id="table-import" data-search="true" data-pagination="true">
          <thead>
              <th>ID</th>
              <th>Matricula</th>
              <th>Operações</th>
          </thead>

          <tbody>
            <% @users.each do |user| %>
            <tr>
              <td><%= user.id %></td>
              <td><%= user.registration %></td>
              <td>
                <%= (link_to 'Editar permissões', admin_user_permission_index_path(user), class: "btn btn-info" ) %>
                 
              </td>
            </tr>
            <% end %>
          </tbody>
        </table>

      </div>
    </div>
  </div>
</div>