<div class="row">
  <div class="col-lg-12">
    <h1 class="page-header">Campi IFPB</h1>
  </div>
</div><!--/.row-->

<div class="row">
  <div class="col-lg-12">
    <div class="panel panel-default">
      <div class="panel-heading">
        <%= link_to('Importar do SUAP', importar_campus_admin_campus_path, class: "btn btn-success pull-right")%>
      </div>
      <div class="panel-body">
        <table class="table table-hover">
          <thead>
            <th>#</th>
            <th>Sigla</th>
            <th>Descrição</th>
            <th>Importado em</th>
            <th>Atualizado em</th>
          </thead>

          <tbody>
            <%- @campus.each do |campi| %>
            <tr>
              <td><%= campi.id %></td>
              <td><%= campi.sigla %></td>
              <td><%= campi.description %></td>
              <td><%= campi.created_at %></td>
              <td><%= campi.updated_at %></td>
            </tr>
            <% end %>
          </tbody>
        </table>
      </div>
    </div>
  </div>
</div>
