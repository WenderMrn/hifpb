

<h1>Aulas da turma: <%= @classroom.name %></h1>
<h4>Clique nas aulas para editar</h4>
<div class="row">
  <div class="col-lg-12">
    <div class="panel panel-default">
      <div class="panel-heading">
        <div class="row">
          <div class="col-md-8">
            <svg class="glyph stroked notepad "><use xlink:href="#stroked-notepad"/></svg> Aulas
          </div>
          <div class="col-md-4">
            <%= link_to 'Adicionar aula', new_admin_classroom_lesson_path, class: "btn btn-success pull-right" %>
          </div>
        </div>
      </div>
      <div class="panel-body">

        <table data-toggle="table" id="table-import" data-search="false" data-pagination="false">
          <thead>
              <th>Horário</th>
              <th>Segunda</th>
              <th>Terça</th>
              <th>Quarta</th>
              <th>Quinta</th>
              <th>Sexta</th>

          </thead>

          <tbody>

        </table>

      </div>
    </div>
  </div>
</div>
